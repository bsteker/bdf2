	{&#xD;
		dorado.MessageBox.alert(&quot;请首先选择用户信息!&quot;);	&#xD;
	}else{&#xD;
		dorado.MessageBox.confirm(&quot;是否确定删除此用户信息?&quot;,function(){&#xD;
				user.remove();&#xD;
				view.id(&quot;updateActionUser&quot;).execute();&#xD;
		});&#xD;
