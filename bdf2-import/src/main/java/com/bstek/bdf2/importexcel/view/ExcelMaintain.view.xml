                <ClientEvent name="onSubControlAction">var excelModelData=this.id(&quot;dataSetExcelModel&quot;).getData();&#xD;
var updateActionExcelModel=this.id(&quot;updateActionExcelModel&quot;);&#xD;
switch(arg.code){&#xD;
	case &quot;+&quot;:&#xD;
		excelModelData.insert({&#xD;
			processorType:1,&#xD;
			startRow:1,&#xD;
			endRow:2&#xD;
		});&#xD;
